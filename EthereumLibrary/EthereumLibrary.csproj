    </EmbeddedResource>
    <EmbeddedResource Include="SmartContract\compiled\IpfsFileStorageLibrary.bin">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="SmartContract\compiled\UsersAndFiles.bin">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="SmartContract\compiled\UsersAndFiles.abi">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </EmbeddedResource>
