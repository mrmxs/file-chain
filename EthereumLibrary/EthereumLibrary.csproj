    </EmbeddedResource>
    <EmbeddedResource Include="SmartContract\compiled\UserStorage.abi">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="SmartContract\compiled\UserStorage.bin">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="SmartContract\compiled\IpfsFileStorageLibrary.abi">
